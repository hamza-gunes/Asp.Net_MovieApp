@model Movie

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Film Listesi</h1>
    <div class="filmler">


        <div class="film1">
            <p>@Model.Title</p>
            <p>@Model.Description</p>
            <p>@Model.Director</p>
            <p>
                <ul>
                    @foreach (var player in Model.Players)
                    {
                        <li>@player</li>
                    }
                </ul>
            </p>
        </div>
    
    
    </div>
</body>
</html>